<!--Objective: custom route making; just click on map to add markers and they will link up.-->
<% provide(:title, "Location") %>

<head>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <% render 'layouts/shim' %>

  <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBDD4iRh-e5YvN2Ip9Xfda7wqLB6mGizxQ&callback=initialize" async defer>
  </script>
</head>

<body class="body_map">
  <%= render 'layouts/header' %>
  <div id ="map">
  </div>
</body>

<%= debug(params) if Rails.env.development? %>


